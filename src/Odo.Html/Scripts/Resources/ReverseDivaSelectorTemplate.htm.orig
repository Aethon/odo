<table cellspacing="0" cellpadding="0">
<tr><td><span data-bind="text: numberAvailable"></span> available</td><td></td><td><span data-bind="text: numberSelected"></span> selected</td></tr>
<tr><td><div local-id="filterPlace" style="display:none"></div></td><td></td><td rowspan="2"><ul local-id="selected" id="selected" tabindex="0"></ul></td></tr>
<tr>
    <td><ul local-id="available" id="available" tabindex="0"></ul></td>
    <td style="vertical-align: top">
        <button local-id="select" type="button" style="display: block" data-bind="enable: canSelect, click: selectFocused" data-button="{ icons: { primary: 'ui-icon-triangle-1-e' }, text: false }">Add</button>
        <button local-id="unselect" type="button" style="display: block" data-bind="enable: canUnselect, click: unselectFocused" data-button="{ icons: { primary: 'ui-icon-triangle-1-w' }, text: false }">Remove</button>
    </td>
</tr>
</table>